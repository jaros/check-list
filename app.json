{
  "expo": {
    "name": "Check List",
    "description": "a list for everything you can imagine",
    "slug": "CheckList",
    "privacy": "public",
    "sdkVersion": "30.0.0",
    "platforms": [
      "ios"
    ],
    "githubUrl": "https://github.com/jaros/check-list",
    "version": "1.0.7",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#a4d792"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.jaros.checklist"
    }
  }
}
